<?xml-stylesheet href="api-default_language.xsl" type="text/xsl"?>


<api.class
    name        = "Error"
    object_name = "error"
    base_dir    = "../"
    author      = "$Author: jz $"
    date        = "$Date: 2006-01-31 12:42:37 +0100 (Tue, 31 Jan 2006) $"
>

<property name="is_error" access="read">
    <com>
        <com.result>
            <com.type type="bool"/>
        </com.result>
    </com>

    <title>
        <code>true</code>, wenn ein Fehler vorliegt
    </title>

</property>


<property name="code" access="read">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>

    <title>
        Der Fehlercode
    </title>
</property>


<property name="text" access="read">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>

    <title>
        Fehlertext (mit Fehlercode)
    </title>
</property>

</api.class>

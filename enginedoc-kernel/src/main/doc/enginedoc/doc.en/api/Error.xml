<?xml-stylesheet href="api-default_language.xsl" type="text/xsl"?>


<api.class
    name        = "Error"
    object_name = "error"
    base_dir    = "../"
    author      = "$Author: jz $"
    date        = "$Date: 2012-06-12 21:42:49 +0200 (Di, 12 Jun 2012) $"
>

<property name="is_error" access="read">
    <com>
        <com.result>
            <com.type type="bool"/>
        </com.result>
    </com>

    <title>
        <code>true</code>, should an error have occurred
    <!--<code>true</code>, wenn ein Fehler vorliegt-->
    </title>

</property>


<property name="code" access="read">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>

    <title>
        The error code
    <!--Der Fehlercode-->
    </title>
</property>


<property name="text" access="read">
    <com>
        <com.result>
            <com.type type="BSTR"/>
        </com.result>
    </com>

    <title>
        The error text (with error code)
    <!--Fehlertext (mit Fehlercode)-->
    </title>
</property>

</api.class>

<xml_element name     = "licence.use"
             parent_page = "../xml_commands.xml"
             category = "Kommando"
             title    = "Deletes a lock"
             base_dir = "../"
             author   = "$Author: jz $"
             date     = "$Date: 2008-10-21 23:20:44 +0200 (Tue, 21 Oct 2008) $">
         <!--title    = "Löscht eine Sperre"-->


    <description>
        <p>
            Adds a license key.
        <!--Fügt einen Lizenzschlüssel hinzu.-->
        </p>
    </description>


    <xml_parent_elements>
        <xml_parent_element name="commands"/>
    </xml_parent_elements>


    <xml_attributes>
        <xml_attribute name="key" type="licence_key" title="Lizenzschlüssel">
            <description>
                <p>
                    The licence key is added to the licence keyring.
                <!--Der Lizenzschlüssel wird dem Lizenzschlüsselbund hinzugefügt.-->
                </p>
                <p>
                    See also <scheduler_ini_entry file="sos.ini" section="licence" entry="key1"/>.
                <!--Siehe auch <scheduler_ini_entry file="sos.ini" section="licence" entry="key1"/>.-->
                </p>
            </description>
        </xml_attribute>
    </xml_attributes>


    <xml_answer element="ok"/>

</xml_element>

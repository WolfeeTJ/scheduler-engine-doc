<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet href="../../scheduler.xsl" type="text/xsl"?>

<!--$Id$-->

<!--         title         = "Antwort"-->

<xml_element name          = "answer" 
             title         = "Answer"
             category      = "Answer"
             sub_directory = "answer/"
             base_dir      = "../../"
             parent_page   = "../../xml_commands.xml"
             author        = "$Author: jz $"
             date          = "$Date: 2012-06-12 21:25:54 +0200 (Di, 12 Jun 2012) $">

    <xml_parent_elements>
        <xml_parent_element name="spooler">
        </xml_parent_element>
    </xml_parent_elements>
    
    <xml_attributes>
        <xml_attribute name="time" type="yyyy-mm-dd hh:mm:ss.mmm">
            <description>
                <p>
                    The answer time stamp.
                <!--Zeitstempel der Antwort.-->
                </p>
            </description>
        </xml_attribute>
    </xml_attributes>
    
    <xml_child_elements>
        <xml_child_element name="ERROR"/>
        <xml_child_element name="history"/>
        <xml_child_element name="job"/>
        <xml_child_element name="job_chain"/>
        <xml_child_element name="order"/>
        <xml_child_element name="process_classes"/>
        <xml_child_element name="ok"/>
        <xml_child_element name="log_categories"/>
        <xml_child_element name="state" title="Answer to&lt;show_state&gt;"/>
    <!--<xml_child_element name="state" title="Antwort auf &lt;show_state&gt;"/>-->
        <xml_child_element name="task"/>
    </xml_child_elements>

    <description>
        <p>
            The JobScheduler only returns one of the child elements.
        <!--Der JobScheduler liefert nur eins der Kind-Elemente.-->
        </p>
    </description>
    
</xml_element>
